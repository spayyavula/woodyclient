# Supabase Configuration (Production)
# Note: These are placeholder values for demo purposes
# In a real deployment, you would replace these with actual Supabase project credentials
VITE_SUPABASE_URL=https://demo.supabase.co
VITE_SUPABASE_ANON_KEY=demo-anon-key

# Stripe Configuration (Production)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_demo_stripe_key

# Application Configuration
VITE_APP_NAME=Rust Cloud IDE
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# API Configuration
VITE_API_BASE_URL=https://api.rustcloudide.com
VITE_WS_URL=wss://ws.rustcloudide.com

# Feature Flags
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_MARKETPLACE=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_MOBILE_PREVIEW=true

# Analytics
VITE_ANALYTICS_ID=your_production_analytics_id

# Production Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error